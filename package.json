{
  "name": "fastify-fauna",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=12.13.1"
  },
  "scripts": {
    "start": "node -r dotenv/config ./dist/index.js",
    "clean": "rimraf ./dist",
    "generate": "ncc build ./src/server.ts --out ./dist --minify",
    "build": "npm-run-all --serial clean generate",
    "dev": "nodemon | pino-pretty",
    "lint": "npm-run-all --parallel lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": "**/*",
    "ext": "ts",
    "exec": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t-mario-y/fastify-fauna.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/t-mario-y/fastify-fauna/issues"
  },
  "homepage": "https://github.com/t-mario-y/fastify-fauna#readme",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.20.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.12.0",
    "faunadb": "^4.1.0"
  }
}
